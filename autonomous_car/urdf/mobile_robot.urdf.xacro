<?xml version="1.0"?>

<robot name="mobile_robot_description" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find autonomous_car)/urdf/simulation_gazebo.xacro"/>

    <!-- Main chassis of car -->
    <link name="base_link"/>
        
    <link name="car_body">
        <inertial>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <mass value="30.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-1 0.5 -0.025" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/badane.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="-1 0.5 -0.025" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/badane.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="car_body"/>
    </joint>

    <!-- Sensors -->
     <!-- Camera -->
    <link name="camera_sensor_link">
        <collision>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
              <box size="0.05 0.05 0.05"/>
          </geometry>
        </collision>
  
        <visual>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <geometry>
              <box size="0.05 0.05 0.05"/>
          </geometry>
        </visual>
  
        <inertial>
          <mass value="1" />
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <joint name="camera_sensor_joint" type="fixed">
        <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
        <parent link="front_steering"/>
        <child link="camera_sensor_link"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <!-- Gyroscope -->
    <link name="IMU_sensor_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
        </collision>
    </link>

    <joint name="IMU_sensor_joint" type="fixed">
        <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="IMU_sensor_link"/>
        <axis xyz="0.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
    </joint>

    <!-- Include Contact sensors URDFs -->
    <!-- <xacro:include filename="front_sensors.urdf.xacro"/>
    <xacro:include filename="rear_sensors.urdf.xacro"/>
    <xacro:include filename="side_sensors.urdf.xacro"/> 
    <xacro:include filename="corner_sensors.urdf.xacro"/> -->

    <!-- Steering of the car -->
    <!-- Back Steering -->
    <link name="back_steering">
        <inertial>
            <origin xyz="0 0 0"  rpy="0.0 0.0 0.0"/>
            <mass value="1.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0 0.0 0.125" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="1.5"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0.0 0.125" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="1.5"/>   
            </geometry>
        </collision>
    </link>

    <joint name="back_steering_joint" type="fixed">
        <origin xyz="-0.85 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="back_steering"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <!-- Front steering -->

    <link name="front_steering">
        <inertial>
            <origin xyz="0 0 0"  rpy="0.0 0.0 0.0"/>
            <mass value="1.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0 0.0 0.125" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="1.5"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0.0 0.125" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.05" length="1.5"/>   
            </geometry>
        </collision>
    </link>

    <joint name="front_steering_joint" type="fixed">
        <origin xyz="0.85 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="front_steering"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <!-- Wheels -->
    <!-- right wheel -->
    <link name="right_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.431"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.25 0.1 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.25 0.1 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <origin xyz="0.85 0.6 0.125" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="right_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <!-- left wheel  -->

    <link name="left_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.431"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.25 0.1 -0.25"  rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.25 0.1 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <origin xyz="0.85 -0.6 0.125" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="left_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <link name="left_back_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.431"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.25 0.10 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.25 0.10 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="left_back_wheel_joint" type="continuous">
        <origin xyz="-0.85 -0.6 0.125" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="left_back_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <link name="right_back_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.431"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.25 0.10 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </visual>
        <collision>
            <origin xyz="-0.25 0.10 -0.25" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <mesh filename="package://autonomous_car/meshes/charkh.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="right_back_wheel_joint" type="continuous">
        <origin xyz="-0.85 0.6 0.125" rpy="0.0 0.0 0.0"/>
        <parent link="car_body"/>
        <child link="right_back_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

</robot>
